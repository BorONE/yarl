$ go run cmd/client/main.go --cmd edit --node-config Id:0;Job:{[type.googleapis.com/register.ShellScriptConfig]:{Path:"test/data/a.sh"}}
$ go run cmd/client/main.go --cmd connect --id 0 --id2 1
